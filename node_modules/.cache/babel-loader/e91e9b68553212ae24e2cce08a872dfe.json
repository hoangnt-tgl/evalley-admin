{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  name: \"Sidebar\",\n  data() {\n    return {\n      width: 0,\n      height: 0\n    };\n  },\n  computed: {\n    ...mapState({\n      menuItems: state => state.menu.data\n    })\n  },\n  created() {\n    window.addEventListener(\"resize\", this.handleResize);\n    this.handleResize();\n  },\n  destroyed() {\n    window.removeEventListener(\"resize\", this.handleResize);\n  },\n  mounted() {\n    this.menuItems.filter(items => {\n      if (items.path === this.$route.path) this.$store.dispatch(\"menu/setActiveRoute\", items);\n      if (!items.children) return false;\n      items.children.filter(subItems => {\n        if (subItems.path === this.$route.path) this.$store.dispatch(\"menu/setActiveRoute\", subItems);\n        if (!subItems.children) return false;\n        subItems.children.filter(subSubItems => {\n          if (subSubItems.path === this.$route.path) this.$store.dispatch(\"menu/setActiveRoute\", subSubItems);\n        });\n      });\n    });\n  },\n  methods: {\n    setNavActive(item) {\n      this.$store.dispatch(\"menu/setNavActive\", item);\n    },\n    handleResize() {\n      this.width = window.innerWidth - 310;\n    }\n  }\n};","map":{"version":3,"mappings":"AAsHA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;IACA;EACA;EACAC;IACAD;EACA;EACAE;IACA;MACA,qCACA;MACA;MACAC;QACA,wCACA;QACA;QACAC;UACA,2CACA;QACA;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","width","height","computed","menuItems","created","window","destroyed","mounted","items","subItems","methods","setNavActive","handleResize"],"sourceRoot":"src/components/partials","sources":["AdminSidebar.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Page Sidebar Start-->\n    <div class=\"page-sidebar\">\n      <div class=\"main-header-left d-none d-lg-block\">\n        <div class=\"logo-wrapper\">\n          <router-link to=\"/dashboard\">\n          <h1 class=\"m-4\">EVALLEY</h1>\n          </router-link>\n        </div>\n      </div>\n      <div class=\"sidebar custom-scrollbar\">\n        <div class=\"sidebar-user text-center\">\n          <div>\n            <img\n              class=\"img-60 rounded-circle lazyloaded blur-up\"\n              src=\"../../assets/images/dashboard/man.png\"\n              alt=\"#\"\n            />\n          </div>\n          <h6 class=\"mt-3 f-14\">JOHN</h6>\n          <p>general manager.</p>\n        </div>\n        <ul\n          class=\"sidebar-menu\"\n          id=\"myDIV\"\n        >\n          <li\n            v-for=\"(menuItem, index) in menuItems\"\n            :key=\"index\"\n            :class=\"{ active: menuItem.active }\"\n          >\n            <!-- Sub -->\n            <a\n              href=\"javascript:void(0)\"\n              class=\"sidebar-header\"\n              v-if=\"menuItem.type == 'sub'\"\n              @click=\"setNavActive(menuItem, index)\"\n            >\n             <feather :type=\"menuItem.icon\"> </feather>\n              <span>\n                {{ menuItem.title }}\n              </span>\n              <i\n                class=\"fa fa-angle-right pull-right\"\n                v-if=\"menuItem.children\"\n              ></i>\n            </a>\n            <router-link\n              :to=\"menuItem.path\"\n              class=\"sidebar-header\"\n              v-if=\"menuItem.type == 'link'\"\n              router-link-exact-active\n            >\n              <feather :type=\"menuItem.icon\" class=\"middle\"></feather>\n              <span>\n                {{ menuItem.title }}\n              </span>\n              <i\n                class=\"fa fa-angle-right pull-right\"\n                v-if=\"menuItem.children\"\n              ></i>\n            </router-link>\n\n            <ul\n              class=\"sidebar-submenu\"\n              v-if=\"menuItem.children\"\n              :class=\"{ 'menu-open': menuItem.active }\"\n            >\n              <li\n                v-for=\"(childrenItem, index) in menuItem.children\"\n                :key=\"index\"\n                :class=\"{ active: childrenItem.active }\"\n              >\n                <!-- Sub -->\n                <a\n                  href=\"javascript:void(0)\"\n                  v-if=\"childrenItem.type == 'sub'\"\n                  @click=\"setNavActive(childrenItem, index)\"\n                >\n                  <i class=\"fa fa-circle\"></i>\n                  {{ childrenItem.title }}\n                  <i\n                    class=\"fa fa-angle-right pull-right\"\n                    v-if=\"childrenItem.children\"\n                  ></i>\n                </a>\n                <!-- Link -->\n                <!--{{ childrenItem.children }}-->\n                <!--{{ childrenItem.path }}-->\n                <span\n                  v-for=\"(item, index) in childrenItem.children\"\n                  :key=\"index\"\n                >\n                </span>\n\n                <router-link\n                  :to=\"childrenItem.path\"\n                  v-if=\"childrenItem.type == 'link'\"\n                  router-link-exact-active\n                >\n                  <i class=\"fa fa-circle\"></i>\n                  {{ childrenItem.title }}\n                  <i\n                    class=\"fa fa-angle-down pull-right\"\n                    v-if=\"childrenItem.children\"\n                  ></i>\n                </router-link>\n              </li>\n            </ul>\n          </li>\n        </ul>\n      </div>\n    </div>\n    <!-- Page Sidebar Ends-->\n  </div>\n</template>\n<script>\nimport { mapState } from \"vuex\";\nexport default {\n  name: \"Sidebar\",\n  data() {\n    return {\n      width: 0,\n      height: 0,\n    };\n  },\n  computed: {\n    ...mapState({\n      menuItems: state => state.menu.data\n    })\n  },\n  created() {\n    window.addEventListener(\"resize\", this.handleResize);\n    this.handleResize();\n  },\n  destroyed() {\n    window.removeEventListener(\"resize\", this.handleResize);\n  },\n  mounted() {\n    this.menuItems.filter(items => {\n      if (items.path === this.$route.path)\n        this.$store.dispatch(\"menu/setActiveRoute\", items);\n      if (!items.children) return false;\n      items.children.filter(subItems => {\n        if (subItems.path === this.$route.path)\n          this.$store.dispatch(\"menu/setActiveRoute\", subItems);\n        if (!subItems.children) return false;\n        subItems.children.filter(subSubItems => {\n          if (subSubItems.path === this.$route.path)\n            this.$store.dispatch(\"menu/setActiveRoute\", subSubItems);\n        });\n      });\n    });\n  },\n  methods: {\n    setNavActive(item) {\n      this.$store.dispatch(\"menu/setNavActive\", item);\n    },\n    handleResize() {\n      this.width = window.innerWidth - 310;\n    }\n  }\n};\n</script> \n<style scoped>\n\n</style>\n"]},"metadata":{},"sourceType":"module"}